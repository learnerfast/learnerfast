{
  "crons": [
    {
      "path": "/api/cron/inactivity",
      "schedule": "0 9 * * *"
    }
  ],
  "headers": [
    {
      "source": "/js/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}
